{
  "@context": "dtmi:dtdl:context;2",
  "@id": "dtmi:embeddedgeorge:environmenthol:photoupload;1",
  "@type": "Interface",
  "displayName": "Photo Upload Module",
  "contents": [
    {
      "@type": "Telemetry",
      "name": "timestamp",
      "schema": "dateTime"
    },
    {
      "@type": "Telemetry",
      "name": "filename",
      "displayName": "Uploaded File Name",
      "schema": "string"
    },
    {
      "@type": "Property",
      "name": "upload_cycle_sec",
      "displayName": "Upload Cycle Sec",
      "schema": "integer",
      "writable": true
    },
    {
      "@type": "Property",
      "name": "current_upload_cycle_sec",
      "schema": "integer",
      "displayName": "Current Upload Cycle Setting Sec"
    },
    {
      "@type": "Property",
      "name":"photo_uploading",
      "displayName": "Photo Uploading Status",
      "schema": "boolean"
    },
    {
      "@type": "Command",
      "name": "Start",
      "description": "Start to take and upload photo file",
      "commandType": "synchronous",
      "request": {
        "name": "photo_upload_notify",
        "displayName": "Upload Notification",
        "schema": {
          "@type": "Object",
          "fields": [
            {
              "name": "photo_upload_notify",
              "displayName": "Is Photo Upload Notify",
              "schema": "boolean"
            }
          ]
        }
      },
      "response": {
        "name": "message",
        "schema": "string"
      }
    },
    {
      "@type": "Command",
      "name": "Stop",
      "description": "Stop to take and upload photo file",
      "commandType": "synchronous",
      "response": {
        "name": "message",
        "schema": "string"
      }
    }
  ]
}
